		"dotenv-cli": "^11.0.0",
		"harperdb": "^4.7.17",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.3.1"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@7.3.1"
	}
